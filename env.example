# Cấu hình môi trường cho Long Châu Crawler

# Cấu hình delay (giây)
REQUEST_DELAY=1
MIN_DELAY=0.5
MAX_DELAY=2.0

# Cấu hình retry
MAX_RETRIES=3
RETRY_DELAY=2

# Cấu hình Selenium (nếu cần)
SELENIUM_TIMEOUT=10
HEADLESS_MODE=true

# Proxy (nếu cần)
# HTTP_PROXY=http://proxy:port
# HTTPS_PROXY=https://proxy:port

# Database (nếu cần lưu vào DB)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=longchau_data
# DB_USER=username
# DB_PASSWORD=password
